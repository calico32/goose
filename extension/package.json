{
  "name": "goose",
  "displayName": "Goose Language Support",
  "description": "",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "vscode": "^1.67.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "generate": "yq syntaxes/goose.tmLanguage.yaml -o=json -P > syntaxes/goose.tmLanguage.json",
    "watch": "watchexec -w syntaxes -e yaml -r \"yarn run generate\"",
    "build": "vsce package",
    "vscode:prepublish": "yarn run generate"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "goose",
        "aliases": [
          "Goose",
          "goose"
        ],
        "icon": {
          "light": "goose.png",
          "dark": "goose.png"
        },
        "extensions": [
          ".goose",
          ".goos"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "goose",
        "scopeName": "source.goose",
        "path": "./syntaxes/goose.tmLanguage.json"
      }
    ]
  }
}
